// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Tweet {
  id         String @id @default(cuid())
  tweetId    String
  authorId   String
  text       String
  source     String
  created_at String
}

model Repo {
  id          String @id @default(cuid())
  name        String
  forks       Int
  open_issues Int
  watchers    Int
}
